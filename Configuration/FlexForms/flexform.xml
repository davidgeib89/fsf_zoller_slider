<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sheet1>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Title</sheetTitle>
                    <sheetDescription>Descr.</sheetDescription>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.references>
                        <section>1</section>
                        <type>array</type>
                        <el>
                            <item>
                                <type>array</type>
                                <title>LLL:EXT:fsf_slider/Resources/Private/Language/locallang.xlf:flexform.slide.title</title>
                                <el>
                                    <headline>
                                        <TCEforms>
                                            <label>Bildtitel (nicht im Frontend sichtbar)</label>
                                            <config>
                                                <type>input</type>
                                                <eval>trim</eval>
                                                <size>30</size>
                                            </config>
                                        </TCEforms>
                                    </headline>
                                    <img>
                                        <TCEforms>
                                            <label>Hintergrund Bild</label>
                                            <config>										
                                                <type>group</type>
                                                <internal_type>file</internal_type>
                                                <allowed>gif, jpg,jpeg,png,csv</allowed>
                                                <max_size>10000</max_size>
                                                <uploadfolder>uploads/tx_fsfzollerslider</uploadfolder>
                                                <maxitems>1</maxitems>
                                                <size>1</size>
                                                <selectedliststyle>Width:180px</selectedliststyle>
                                            </config>
                                        </TCEforms>
                                    </img>
                                </el>
                            </item>
                        </el>
                    </settings.references>
                </el>
            </ROOT>
        </sheet1>
    </sheets>
</T3DataStructure>
